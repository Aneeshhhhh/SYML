#include <iostream>
#include <queue>
using namespace std;

int g[10][10], visited[10], n;

void bfs(int start) {
    queue<int> q;
    q.push(start);
    visited[start] = 1;

    while (!q.empty()) {
        int u = q.front();
        q.pop();
        cout << u << " ";

        for (int v = 0; v < n; v++)
            if (g[u][v] == 1 && !visited[v]) {
                visited[v] = 1;
                q.push(v);
            }
    }
}

void dfs(int u) {
    visited[u] = 1;
    cout << u << " ";
    for (int v = 0; v < n; v++)
        if (g[u][v] == 1 && !visited[v])
            dfs(v);
}

int main() {
    cin >> n;
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            cin >> g[i][j];

    for (int i = 0; i < n; i++) visited[i] = 0;
    bfs(0);
    cout << endl;

    for (int i = 0; i < n; i++) visited[i] = 0;
    dfs(0);
}
