#include <iostream>
using namespace std;

struct Node {
    int data;
    Node* next;
};

Node* head = NULL;

void insertBegin(int x) {
    Node* t = new Node;
    t->data = x;
    t->next = head;
    head = t;
}

void insertEnd(int x) {
    Node* t = new Node;
    t->data = x;
    t->next = NULL;

    if (head == NULL) {
        head = t;
        return;
    }

    Node* p = head;
    while (p->next != NULL)
        p = p->next;

    p->next = t;
}

void display() {
    Node* p = head;
    while (p != NULL) {
        cout << p->data << " ";
        p = p->next;
    }
}

int main() {
    insertBegin(10);
    insertEnd(20);
    insertEnd(30);
    insertBegin(5);
    display();
}
