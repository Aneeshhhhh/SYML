#include <iostream>
using namespace std;

class Stack {
    int a[100], top;
public:
    Stack() { top = -1; }

    void push(int x) {
        if (top < 99)
            a[++top] = x;
    }

    int pop() {
        if (top >= 0)
            return a[top--];
        return -1;
    }

    void display() {
        for (int i = top; i >= 0; i--)
            cout << a[i] << " ";
    }
};

int main() {
    Stack s;
    s.push(10);
    s.push(20);
    s.push(30);
    s.pop();
    s.display();
}
